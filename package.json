{
  "name": "questions",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android:release": "npx react-native run-android --variant=release",
    "android-clean-build": "cd android && ./gradlew clean && cd ..",
    "android-build-debug": "cd android && ./gradlew assembleDebug && cd ..",
    "run:android": "yarn rebuild:android && yarn android",
    "rebuild:android": "yarn clean:android && yarn build:android",
    "clean:android": "rm -rf android/app/build && rm -rf android/build && rm -rf android/.gradle && rm -rf android/build.gradle-e",
    "build:android": "yarn && npx jetifier",
    "ios": "npx react-native run-ios",
    "run:ios:rebuild": "yarn rebuild:ios && yarn ios",
    "run:ios:nodemodules": "yarn rebuild:nodemodules && yarn ios",
    "rebuild:ios": "yarn clean:nodemodules && yarn clean:ios && yarn build:ios",
    "clean:ios": "rm -rf ios/build && rm -rf ios/Pods && yarn clean:ios:pods",
    "clean:ios:pods": "cd ios && pod deintegrate && pod cache clean --all && cd ..",
    "build:ios": "yarn && cd ios/ && pod install && cd ..",
    "rebuild:nodemodules": "yarn clean:nodemodules && yarn build:nodemodules",
    "clean:nodemodules": "rm -rf node_modules && rm -f yarn.lock",
    "build:nodemodules": "yarn install",
    "rebuild": "yarn clean && yarn build",
    "clean": "yarn clean:nodemodules && yarn clean:android && yarn clean:ios",
    "build": "yarn && yarn build:android && yarn build:ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "axios": "^0.27.2",
    "immer": "^9.0.14",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-svg": "^12.3.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^27.1.5",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-jest": "^27.1.5",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.1.5",
    "metro-babel-register": "^0.71.1",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "17.0.2",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^27.1.5",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
